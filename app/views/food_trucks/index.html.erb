<p id="notice"><%= notice %></p>

<h1>Listing Food Trucks</h1>

<table>
  <thead>
    <tr>
      <th>Foodtruckid</th>
      <th>Merchantuseremail</th>
      <th>Name</th>
      <th>Type</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_trucks.each do |food_truck| %>
      <tr>
        <td><%= food_truck.foodTruckID %></td>
        <td><%= food_truck.merchantUserEmail %></td>
        <td><%= food_truck.name %></td>
        <td><%= food_truck.type %></td>
        <td><%= food_truck.longitude %></td>
        <td><%= food_truck.latitude %></td>
        <td><%= link_to 'Show', food_truck %></td>
        <td><%= link_to 'Edit', edit_food_truck_path(food_truck) %></td>
        <td><%= link_to 'Destroy', food_truck, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food truck', new_food_truck_path %>
